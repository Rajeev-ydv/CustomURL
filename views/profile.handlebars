<h1> Profile </h1>
<b> {{user.username}} <p>

{{#if urls}}
 <div class="table-responsive">          
  <table class="table">
    <thead>
      <tr>
        <th>ShortCode</th>
        <th>URL</th>
        <th>Hits</th>
        <th>Created On</th>
      </tr>
    </thead>
    <tbody>
    {{#each urls}}
     <tr>
      <td> {{shortcode}} </td>
      <td> {{url}} </td>
      <td> {{owner}} </td>
      <td> {{created_at}} </td>
    </tr>
    {{/each}}
   </tbody>
  </table>
 </div>
{{/if}}
